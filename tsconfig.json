{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "outDir": "generators",
        "rootDir": "src"
    },
    "exclude": [
        "node_modules",
        "src/app/templates",
        "generators"
    ]
}
